    <script type="text/javascript">
    // Fonction pour l'ouverture du menu
    function sn_fpOpenMenu (save) {
        if($('.snfp_logo').hasClass('openned')){
            $( ".snfp_side_menu" ).animate({
                left: "-240px",
                opacity:0,
              }, 300, function() {
              });
            $( ".snfp_logo" ).animate({
                left: "10px"
              }, 300, function() {
              });
			  
			$('body').addClass('snfp_side_menu_off');
        }

        if($('.snfp_logo').hasClass('closed')){
             $( ".snfp_side_menu" ).animate({
                left: "0px",
                opacity:0.95,
              }, 300, function() {
              });
             $( ".snfp_logo" ).animate({
                left: "210px"
              }, 300, function() {
              });
			  
			$('body').removeClass('snfp_side_menu_off');
        }
        $('.snfp_logo').toggleClass('openned');
        $('.snfp_logo').toggleClass('closed');
        $('.snfp_side_menu').toggleClass('openned');
		if (save) {
			state = $('.snfp_logo').hasClass('openned');
			sn_run_op("op=set&var=menuopen_state&state="+state, "Can't save state of menu : ");
		}
    }

	function sn_run_op(data, errmsg, callback) {
		$.ajax({//
			url: "/extenso/extranet/fr/fullpage_ajax.snc",
			data: data,
			dataType: 'JSON',
			success: function(ret){//
				if(ret.retcode != 0) {//
					alert(ret.retmesg);
				} else {
					if (callback != undefined) {
						callback(ret);
					}
				}
			},
			error: function(a,b,c) {
				alert(errmsg + a + b + c);
			}
		});
	}
	
