    <script>
        function sizeLimit() {
            var total_size = 0;
            $('input[name=file]').each(function(){
                if(this.files[0] !== undefined){
                    total_size = total_size + this.files[0].size; 
                }
            });
            
            $('#uploaded_size').val(total_size);
