	function sn_load(url, options) {
		container = $("body").data("sn_id");
		console.log("container="+container);
		var defaults = { // Defaults values for options
			'id' : container,
			'iframe' : false
		};
		var opts = jQuery.extend(defaults, options);
		//opts.id = container;
		if (opts.id == undefined) {
            $('.sn_slide_panel').show().addClass('sn_panel_on');
			opts.id = "#sn_container";
		}
		console.log("sn_load in " + opts.id);
	
		if (opts.iframe) {
			console.log("Load in iframe ");
			$(opts.id).html("<iframe class='index_iframe' src='"+url+"'><iframe>");
		} else {
			$(opts.id).css('opacity','0.5');
			$.ajax({
				url:url,
				success:function(data) {
					console.log("Loading in "+opts.id);
					$(opts.id).css('opacity','1.0');
					$(opts.id).html(data);
					//console.log("Success");
				},
				error:function(data) {
					$(opts.id).html(data.responseText);
					$(opts.id).css('opacity','1.0');
					//console.log("Error");
				}
			});
		}
	}

	function sn_load_panel(url, options) {
		var defaults = { // Defaults values for options
				'id' : '#sn_container',
			'iframe' : false
		};
		var opts = jQuery.extend(defaults, options);
		$("body").data("sn_id",opts.id); // Set which container
	
		$('.sn_slide_panel').fadeIn("fast", "linear").addClass('sn_panel_on');
		$('body').addClass('sn_slide_panel_on');
	
		if (opts.iframe) {
			console.log("Load in iframe ");
			$(opts.id).html("<iframe class='index_iframe' src='"+url+"'><iframe>");
		} else {
			$(opts.id).css('opacity','0.5');
			$.ajax({
				url:url,
				success:function(data) {
					console.log("Loading in "+opts.id);
					$(opts.id).css('opacity','1.0');
					$(opts.id).html(data);
					//console.log("Success");
				},
				error:function(data) {
					$(opts.id).html(data.responseText);
					$(opts.id).css('opacity','1.0');
					//console.log("Error");
				}
			});
		}
	}

    function closeSlidePanel() {
        $('.sn_slide_panel').fadeToggle("fast", "linear").removeClass('sn_panel_on');
		$('body').removeClass('sn_slide_panel_on');
    }

	function sn_publish_current_page() {
		//console.log("page=311");
		var uid = "311";
		var publish = "";
		var arr_href = window.location.href.split("/");
		var new_href = "", separ_href="";
	
		if (uid != undefined) {
			data = "op=publish&uid="+uid+"&publish="+publish;
			$.ajax({//
				url: "/extenso/extranet/fr/fullpage_ajax.snc",
				data: data,
				dataType: 'JSON',
				success: function(ret){//
					if(ret.retcode != 0 || ret.retmesg != "") {//
						alert(ret.retmesg);
					} else {
						if (confirm("La publication est terminée. Voulez-vous recharger la page ?")) {
							for(i=0;i<arr_href.length;i++){//
								if (!arr_href[i].match(/#.*/)){//
									new_href = new_href + separ_href + arr_href[i];
									separ_href = "/";
								}
							} 
							window.location = new_href;
						}
					}
				},
				error: function(a,b,c) {
					alert("Can't publish page : " + a + b + c);
				}
			});
		}
	}
	
