<!DOCTYPE html>
{{ 
    use lg;         // Function publish use context lg
    use page;       // Function publish use context page for the content of the page
    use user;       // From sn_preload
    use grid;       // In sn_publish grid is initialize 

    %include "/extenso/functions/sn_ct.snc"; 
    %include "/extenso/functions/sn_widgets.snc"; 
	%include "/extenso/functions/sn_pages.snc";
	
	if page.rows{'page_' .+ lg.rows.lg} ne "index" then
		pageurl = page.rows{'folder_' .+ lg.rows.lg} .+ page.rows{'page_' .+ lg.rows.lg};
	else
		pageurl = page.rows{'folder_' .+ lg.rows.lg};
	endif // use by menus
}}
\{{ 
    config=config(); 
	page={	"uid":"{{page.rows.uid}}", 
			"role":"{{page.rows.role}}",
			"code":"{{page.rows.code}}",
			"url":"{{pageurl}}" 
			{{ for l sql("select * from sn_languages where active = 'yes'") do }}
				,"url_{{l.rows.lg}}":"{{page.rows{'folder_' .+ l.rows.lg};
				if page.rows{'page_' .+ l.rows.lg} ne "index" then
					page.rows{'page_' .+ l.rows.lg};
				endif
			}}"
			{{ endfor }}
	}; 
}}
<html lang="{{lg.rows.lg}}">
    <head>
        {{ %include "/includes/header.sn"; }}
        <!-- Generated by {{ 
                user.username; " " ; datetime();  " "; 
                files = filenames(); 
                for file in files.filenames do 
                    file; " ;  "; 
                    endfor 
        }} -->
    </head>
    <body class="site">
        \{{ 
            if config.site eq "staging" then 
                client_has_bootstrap = true; 
                if user.lg ne "" then
                    include("/extenso/html/extranet/" .+ user.lg .+ "/fullpage.snc");
                else
                    include("/extenso/html/extranet/fr/fullpage.snc"); 
                endif
            endif
        }}
        <div class="cs sed">
		 	<div class="sed-content">
		 	    
		 	    
                <div class="widget_area_top">
			    	{{ sn_widgets(add:true,table:"pages",uid:page.rows.uid,lg:lg,position:"haut"); }}
                </div>
                
                <div class="container">
                    <div class="row">
                        <div class="sn-grid col-md-12">
    				    	{{ sn_widgets(add:true,table:"pages",uid:page.rows.uid,lg:lg,position:"center"); }}
    					</div>
					</div>
                </div>

              
              
              
              <!--start of footer-->
{{ 
   widget_code = "sed_counter";
    widget = sql(single:true, "SELECT * FROM sn_widgets WHERE widget_code = '?'", widget_code);
    
    if widget.rows.widget_global eq "yes" then
        include("/" .+ config.site .+ "/widgets/" .+ lg.rows.lg .+ "/" .+ widget.rows.widget_code .+ ".snc");
    elseif widget.rows.publish eq "yes" then
        read("/" .+ config.site .+ "/widgets/" .+ lg.rows.lg .+ "/" .+ widget.rows.widget_code .+ ".snc");
    else
    	read(widget.rows.code);
    endif
}}
\{{  //Cookie add
//use extranet_user;

// Initialise cookie
// pour test ?region=mtl&net=g&cre=146204715289&campaignid=250098300&adgroupid=18001996140&pos=1t4&kw=Entrepreneur Général
//  et regarder dans le projet

cookies = cookies();
request = request();

if cookies.soumissionrenovation == undefined then
    if (request.args) then     // test si contient cre
       // if strpos(request.args,"cre") >= 0 then 
        	date = datetime();
        	adwords = request.args;
        	cookie = { 'url' : request.hostname .+ request.uri, 'adwords' : adwords, 'date' : date };
        	expirytime=datetime(extended:true,op:"+3h",format:"%Sednove2");
        	cookies(name:"soumissionrenovation",value:cookie,path:"/",expiry:expirytime);
        //endif
    endif
endif
if cookies.first_visited == undefined then
	expiry=datetime(extended:true,op:"+3h",format:"%Sednove2");
	cookies(name:"first_visited",value:request.hostname .+ request.uri,path:"/",expiry:expiry);
endif
}}

<div class="container-full sed-footer no-print">
	<div class="container-fluid">            
		<div class="row">
			<div id="sn_cs_footer">
			    {{
		            /*if footer_grid.nbrows > 0 then
		                uid_grid = footer_grid.rows.uid;
		                grid = unstringnify(footer_grid.rows.grid);
			            %include "/includes/show_grid.sn";
		            endif*/
			    }}
			    {{
                    use config;
                    
                    logo = sql(single:true, "SELECT sn_configs.logo_fr,sn_configs.logo_en FROM sn_configs WHERE uid = 1");
                    if lg.rows.lg eq 'fr' then
                        logo_url = logo.rows.logo_fr;
                    else
                        logo_url = logo.rows.logo_en;
                    endif
                }}
                <div class="container-fluid footer">
                    <div class="logo">
                       <a   href="{{pages("accueil", lg.rows.lg,table:"pages")}}"     > <img src="{{logo_url}}" alt="Soumission Rénovation"></a>
                    </div>
                    <div class="footer_towns">
                        {{sn_ct("footer towns", lg:lg.rows.lg, publish:"/extenso/publish/ressources.sn", uids:ressource.rows.uid)}}
                    </div>
                    <div class="footer_copy">
                        © {{datetime(format:"%Y");}} {{sn_ct("footer copyright", lg:lg.rows.lg, publish:"/extenso/publish/ressources.sn", uids:ressource.rows.uid)}}
                    </div>
                    <div class="row">

                        <div  class="col-sm-8">
                             <div class="mt-sm menu_footer_align_right secondary_menu">
                                {{ %include "/includes/call_footer_nav_extranet.sn"; }}
                            </div>
                        </div>

                    </div>
                              
                </div><!--end .footer-->
                <style>
                    .footer .secondary_menu{
                        font-size:12px;
                    }
                    .footer .row.border-top{
                        border-top:6px #dfdfdf solid;
                        padding-top:42px;
                        margin-top:30px;
                    }
                    .footer .logo{
                        text-align:center;
                        margin:0 0 30px 0;
                    }
                    .footer .logo img{
                        max-height:120px;
                        max-width:220px;
                    }
                    .footer_towns{
                        font-weight:bold;
                        text-align:center;
                    }
                    .footer_copy{
                        font-size:10px;
                        text-align:center;
                    }
                    .footer .menu{
                        margin:10px 0;
                    }
                    
                    .footer .item{
                        display:inline-block;
                        margin-right:15px;
                        line-height:2;
                    }
                    .footer .menu_footer_align_right{
                        text-align:right;
                    }
                    .footer .menu_footer_align_right .item{
                        display:inline-block;
                        margin:0 0 0 15px;
                    }
                    .footer .soc_media{
                        font-size:26px;
                    }
                    .footer .soc_media a{
                        text-decoration:none;
                    }
                    .footer .signature{
                        line-height:37px;
                    }
                    .button_container .btn{
                        margin-bottom:7px;
                    }
                    .button_container .btn:hover{
                        text-decoration:none;
                    }
                    .button_container .btn i{
                        font-size:10px;
                    }
                    .clearfix:after {
                     visibility: hidden;
                     display: block;
                     font-size: 0;
                     content: " ";
                     clear: both;
                     height: 0;
                     }
                     .dt-pull-right{
                         float:right;
                     }
                     .dt-text-right{
                         text-align:right;
                     }
                     .block-mob-center{
                         text-align:left;
                     }
                     .mob-center{
                         text-align:left;
                     }
                     @media (max-width:768px){
                        .footer .menu_footer_align_right{
                            text-align:center;
                        }
                        .footer .menu_footer_align_right .item{
                            display:inline;
                            margin:0 7px;
                        }
                         .dt-pull-right{
                             float:none;
                         }
                         .dt-text-right{
                             text-align:center;
                         }
                         .block-mob-center{
                             display:inline-block;
                         }
                         .mob-center{
                             text-align:center;
                         }
                         .soc_media a{
                             margin:0 4px;
                         }
                         .button_container .btn{
                            margin-bottom:8px;
                            margin-right:4px;
                            margin-left:4px;
                        }
                        .button_container .btn:hover{
                            text-decoration:none;
                        }
                        .signature{
                            padding:10px 0 0 0;
                        }
                     }
                     @media (max-width:500px){
                         .button_container .btn{
                             display:block;
                             margin:0 0 7px 0;
                         }
                     }
                </style>
			</div>
		</div>
	</div>
</div>
<script>
    //Smooth scrolling
    $(function() {
        $('a[href*="#"]:not([href="#"]):not(.open-popup-link)').click(function() {
            if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
                var target = $(this.hash);
                target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
                if (target.length) {
                    $('html, body').animate({
                        scrollTop: target.offset().top
                    }, 1000);
                    return false;
                }
            }
        });
    });
</script>
 
