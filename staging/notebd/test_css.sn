<script type="text/javascript">
	function completeSocialAuthentication(response) {
		console.dir(response);
		var status = response.status;
		if (status == "success") {
			var ssoAction = response.ssoAction;
	
			deleteCookies();
			
			if (ssoAction == "register") {
				var storeId = response.storeId;
				var catalogId = response.catalogId;
				var langId = response.langId;
				var fbUserId = response.fbUserId;
				var logonId = response.logonId;
				var logonPassword = response.logonPassword;
				var logonPasswordVerify = response.logonPasswordVerify;
				var displayName = response.displayName;
				var firstName = response.firstName;
				var lastName = response.lastName;
				var email1 = response.email1;
				var country = response.country;
				var fbPicture = response.fbPicture;
				var url = "https://www.shop.ca/Welcome?catalogId=10301&langId=-1&storeId=10201";
				
				submitSingleSignOnRegisterRequest("post", "/webapp/wcs/stores/servlet/ShopSSOView", ssoAction, storeId, catalogId, langId, fbUserId, logonId, logonPassword, logonPasswordVerify, displayName, firstName, lastName, email1, country, fbPicture, url);
			} else if (ssoAction == "logon") {
				var storeId = response.storeId;
				var catalogId = response.catalogId;
				var langId = response.langId;
				var fbUserId = response.fbUserId;
				var logonId = response.logonId;
				var logonPassword = response.logonPassword;

				var currentURL = document.location.href;
				var curRedirectURL = "";
				if(currentURL.indexOf('ShopRegistrationLandingLogonForm') == -1 && currentURL.indexOf('LogonForm') == -1 && curRedirectURL.indexOf('OrderShippingBillingView') == -1 && curRedirectURL.indexOf('OrderItemMove') == -1){
					var url = currentURL;
				}				
				else if(curRedirectURL.indexOf('OrderShippingBillingView') != -1 || curRedirectURL.indexOf('OrderItemMove') != -1){
					var url = curRedirectURL;
				}
				else{
					var url = 'http://' + document.domain;
				}
				
				submitSingleSignOnLogonRequest("post", "/webapp/wcs/stores/servlet/ShopSSOView", ssoAction, storeId, catalogId, langId, fbUserId, logonId, logonPassword, url);
			}
		} else {
			alert("An error has occurred. Please try again. If the error persists, please contact the site administrator.");
		}
	
		return;
	}
	
	function deleteCookies() {
		//pluck forwards back cookies that were set when accessing the underlying command
		//delete any session based cookies that may have been set
		var cookies = document.cookie.split(";");
		var index;
		for (var index = 0; index < cookies.length; index++) {
			cookieEntry = cookies[index];
			while (cookieEntry.charAt(0) == " ") {
				cookieEntry = cookieEntry.substring(1, cookieEntry.length);
			}
			if (cookieEntry.indexOf("WC_ACTIVEPOINTER") == 0 || cookieEntry.indexOf("WC_GENERIC_ACTIVITYDATA") == 0 || cookieEntry.indexOf("WC_SESSION_ESTABLISHED") == 0 || cookieEntry.indexOf("WC_USERACTIVITY_") == 0 || cookieEntry.indexOf("WC_PERSISTENT") == 0 || cookieEntry.indexOf("ECOMWEB") == 0 || cookieEntry.indexOf("JSESSIONID") == 0) {
				deleteCookie(cookieEntry.substring(0, cookieEntry.indexOf("=")));
			}
		}
	}
	
	function deleteCookie(name) {
		var value = "";
		var date = new Date();
		date.setTime(date.getTime()+(-1*24*60*60*1000));
		var expires = "; expires=" + date.toGMTString();
		var domain = "; domain=" + ".www.shop.ca";
		var path = "; path=/";
		document.cookie = name + "=" + value + expires + domain + path;
	}
	
	function submitSingleSignOnRegisterRequest(method, action, ssoAction, storeId, catalogId, langId, fbUserId, logonId, logonPassword, logonPasswordVerify, displayName, firstName, lastName, email1, country, fbPicture, url) {
		var form = document.createElement("form");
		form.setAttribute("method", method);
		form.setAttribute("action", action);
	
		var hiddenField00 = document.createElement("input");
		hiddenField00.setAttribute("type", "hidden");
		hiddenField00.setAttribute("name", "ssoAction");
		hiddenField00.setAttribute("value", ssoAction);
